---
import AlternativeLayout from "../../components/AlternativeLayout.astro";
import BlogPage from "../../components/react/BlogPage";
const apiUrl = import.meta.env.PUBLIC_API_URL;
---

<!-- Render on a most convenient way the blog entry using markdown coming from github gists -->
<AlternativeLayout headerProps={{ title: "Blog Post" }}>
    <BlogPage apiUrl={apiUrl} client:only="react" />
</AlternativeLayout>
